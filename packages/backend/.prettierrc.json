{
  "useTabs": false,
  "endOfLine": "auto",
  "printWidth": 80,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "htmlWhitespaceSensitivity": "strict",
  "jsxBracketSameLine": false,
  "bracketSpacing": true,
  "insertPragma": false,
  "requirePragma": false,
  "quoteProps": "as-needed",
  "eslintIntegration": true,
  "parser": "typescript",
  "jsxSingleQuote": false,
  "vueIndentScriptAndStyle": false,
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true
}